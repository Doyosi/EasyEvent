{
  "name": "doyosi/easy-event",
  "description": "Simple, extensible event manager for Laravel.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Doyosi\\EasyEvent\\": "src/"
    },
    "classmap": [
      "database/seeders",
      "database/factories"
    ],
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": ["Doyosi\\EasyEvent\\EasyEventServiceProvider"],
      "aliases": { "EasyEvent": "Doyosi\\EasyEvent\\Facades\\EasyEvent" }
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "illuminate/database": "^12.0",
    "illuminate/console": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/view": "^12.0",
    "illuminate/http": "^12.0",
    "spatie/laravel-translatable": "^6.11",
    "spatie/laravel-translation-loader": "^2.8"
  }
}
